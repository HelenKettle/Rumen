#use all defaults

rm(list=ls())
graphics.off()

library(microPop)
library(devtools)

load_all('microPopRumen')

timeStep=1/60 #one minute
times.h=seq(0,24,by=timeStep)


#RATE FUNCS
out=rumenModel(
    times.h=times.h,
    microbeNames=microbeNames,
    resourceSysInfo=sys.res,
    microbeSysInfo=sys.bac,
    rumenRateFuncs=rumenRateFuncs,
    additive=additive,
    basal.diet=diet,
    spinUpTime.hours=spinUpTime.hours,
    useNetworkFuncs=useNetworkFuncs,
    feedMat=feedMat,
    gasMat=gasMat,
    init.with.CH4.data=init.with.CH4.data,
    dietCompositionMat=dietCompositionMat,
    paramList=paramList
)

quickPlot(out)

plotCompareGas(out,spinUpTime.hours,gasMat)


    

